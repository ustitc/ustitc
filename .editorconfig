root = true

[*]
charset = utf-8
indent_size = 4
indent_style = space

[*.java]
ij_java_class_count_to_use_import_on_demand = 999
ij_java_continuation_indent_size = 2
ij_java_generate_final_locals = true
ij_java_method_call_chain_wrap = split_into_lines
ij_java_method_parameters_wrap = split_into_lines
ij_java_record_components_wrap = split_into_lines